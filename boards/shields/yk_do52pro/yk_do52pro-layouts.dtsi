            , <&key_physical_attrs 100 100 1500  175       0     0     0>
            //middle letter row
            , <&key_physical_attrs 100 100    0  275       0     0     0>
            , <&key_physical_attrs 100 100  100  250       0     0     0>
            , <&key_physical_attrs 100 100  200  225       0     0     0>
